{
  "name": "pypi-proxy",
  "type": "proxy",
  "format": "pypi",
  "url": "https://pypi.org/",
  "online": true,
  "storage": {
    "blobStoreName": "default",
    "strictContentTypeValidation": true
  },
  "proxy": {
    "remoteUrl": "https://pypi.org/",
    "contentMaxAge": 1440,
    "metadataMaxAge": 1440
  },
  "httpClient": {
    "blocked": false,
    "autoBlock": true,
    "connection": {
      "useTrustStore": false
    }
  },
  "negativeCache": {
    "enabled": true,
    "timeToLive": 1440
  }
}