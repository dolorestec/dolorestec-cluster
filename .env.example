# Environment variables for Dolorestec Cluster
# Copy this file to .env and fill in the actual values

POSTGRES_USER=user
POSTGRES_PASSWORD=your_postgres_password_here
POSTGRES_DB=dolorestec_db

REDIS_PASSWORD=your_redis_password_here

RABBITMQ_DEFAULT_USER=rabbitmq_user
RABBITMQ_DEFAULT_PASS=your_rabbitmq_password_here

GITHUB_TOKEN=your_github_personal_access_token_here
GITHUB_RUNNER_TOKEN=your_github_runner_token_here
GITHUB_OWNER=dolorestec
GITHUB_REPOSITORY=dolorestec-cluster

# Runner environment variables
RUNNER_TOKEN=your_runner_token_here
REPO_URL=https://github.com/dolorestec/dolorestec-cluster
RUNNER_NAME=dolorestec-runner
RUNNER_WORKDIR=_work